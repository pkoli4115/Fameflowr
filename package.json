  "scripts": {
    "dev": "cd admin && npm run dev",
    "build": "cd admin && npm run build",
    "serve": "firebase serve",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:functions": "cd functions && npm run build && firebase deploy --only functions",
    "deploy:all": "npm run build && firebase deploy",
    "seed:aggregates": "cd functions && npm run seed:aggregates"
  },
  "dependencies": {
    "firebase-admin": "^13.4.0",
